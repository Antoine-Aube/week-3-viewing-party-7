<h2><%= @user.name %>'s Dashboard</h2>
<% if current_user %>
    <p> Currently logged in as: <%= current_user.email %> </p>
    <%= link_to "Log out", logout_path %>
<% end %>

<%= button_to "Find Top Rated Movies", movies_path(@user), method: :get %><br>

<h3>Viewing Parties:</h3>

<% @user.viewing_parties.each do |vp| %>
    Title: <%= vp.movie.title %>
    Duration: <%= vp.duration %>
    Date: <%= vp.date %>
    Time: <%= vp.time %>
<% end %>